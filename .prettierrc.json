{
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "trailingComma": "none",
  "printWidth": 100,
  "tabWidth": 2,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "useTabs": false,
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "preserve",
  "quoteProps": "as-needed"
}
